snippet "frame" "begin{frame} / end{frame}" br
\begin{frame}
	\frametitle{${1:title}}

	${2:Headline is typed here}
	\vspace{2em}

	${0:${VISUAL}}
\end{frame}
endsnippet

snippet graph "graphics" b
\begin{center}
	\includegraphics[width=${1:1.0}\linewidth]{${2:name}}
\end{center}
endsnippet

snippet vspace "vspace" b
\vspace{{1:2}em}
endsnippet
