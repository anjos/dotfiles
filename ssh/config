Host *
  ForwardAgent yes
  StrictHostKeyChecking no
  GSSAPIAuthentication yes
  GSSAPIDelegateCredentials yes
  ServerAliveInterval 60
  ControlMaster auto
  ControlPath /tmp/%r@%h-%p
  ControlPersist 600

Host orquidea
  ForwardX11 yes
  ForwardX11Trusted yes
  ForwardAgent yes
  StrictHostKeyChecking no

# Not proxying for the portal
Host login.idiap.ch
  ProxyCommand none

# Setup Idiap proxying for internal hosts
Host *.idiap.ch
  ForwardX11 no
  ForwardX11Trusted no
  Compression yes
  CompressionLevel 9
  ForwardAgent yes
  ProxyCommand ssh aanjos@login.idiap.ch netcat -w 1 %h %p 2> /dev/null

Host github.com
  ForwardX11 no
  ForwardX11Trusted no
  Compression yes
  CompressionLevel 9

# allows 'ssh idiap ...'
Host idiap
  Hostname italix10.idiap.ch
  Compression yes
  CompressionLevel 9
  ForwardAgent yes
  ProxyCommand ssh aanjos@login.idiap.ch netcat -w 1 %h %p 2> /dev/null

Host win
  User andre
  Hostname localhost
  Port 2207
  Compression no
  ForwardAgent yes
  StrictHostKeyChecking no
