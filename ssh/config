# Order matters - ssh will only use options which are not previously
# defined inside this configuration file.

Host orquidea nas
  User andre
  Hostname orquidea.local

Host github github.com
  User git
  ForwardAgent no
  Hostname github.com

Host gitlab gitlab.idiap.ch
  User git
  ForwardAgent no
  Hostname gitlab.idiap.ch

# Default proxy jump host at Idiap
Host proxy
  User aanjos
  Hostname login.idiap.ch
  ProxyCommand none

Host idiap
  User aanjos
  Hostname italix22.idiap.ch
  ProxyCommand ssh proxy nc %h %p

Host cimacosx beatmacosx01 beatmacosx01.lab beatmacosx01.lab.idiap.ch
  User aanjos
  Hostname beatmacosx01.lab.idiap.ch
  ProxyCommand ssh proxy nc %h %p

Host cilinux bpr bpr-facedemo bpr-facedemo.lab bpr-facedemo.lab.idiap.ch
  User root
  Hostname bpr-facedemo.lab.idiap.ch
  ProxyCommand ssh proxy nc %h %p

Host ubu
  User root
  Hostname beatubulatest.lab.idiap.ch
  ProxyCommand ssh proxy nc %h %p

Host beat* beat*.lab.idiap.ch
  User root
  Hostname %h.lab.idiap.ch
  ProxyCommand ssh proxy nc %h %p

Host *
  Compression yes
  CompressionLevel 9
  ForwardAgent yes
  ForwardX11 no
  ForwardX11Trusted no
  ServerAliveInterval 60
  ControlMaster auto
  ControlPath /tmp/%r@%h-%p
  ControlPersist 600
